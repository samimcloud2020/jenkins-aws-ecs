{
    "family": "my-first-task",
    "containerDefinitions": [
        {
            "image": "291222035571.dkr.ecr.us-east-1.amazonaws.com/my-first-ecr-repo",
            "name": "my-first-task",
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3100,
                    "hostPort": 3100,
		    "protocol": "tcp"	
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::291222035571:role/ecsTaskExecutionRole"
}
